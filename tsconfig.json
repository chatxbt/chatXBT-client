{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "allowImportingTsExtensions": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "baseUrl": ".",
    "paths": {
      "@components/*" : ["components/*"],
      "@styles/*" : ["styles/*"],
      "@chatxbt-sdk/*" : ["chatxbt-sdk/*"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", "chatxbt-sdk/utils/debounce/index.js", "chatxbt-sdk/hooks/utilityAndConfig/index.js"],
  "exclude": ["node_modules"]
}
